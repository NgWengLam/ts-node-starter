{
  "all": true,
  "branches": 50,
  "check-coverage": true,
  "exclude": ["**/*.d.ts", "**/*.test*.ts", "**/index.ts", "**/demo/**/*"],
  "extension": [".ts"],
  "functions": 50,
  "include": ["src/**/*.ts"],
  "lines": 50,
  "per-file": false,
  "reporter": ["html", "lcov"],
  "require": ["ts-node/register"],
  "skip-full": true,
  "statements": 50
}
